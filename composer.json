{
  "name": "blazervel/auth",
  "description": "Ready-to-go authentication (via Laravel Fortify) for Blazervel",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.0",
      "illuminate/support": "^8.67|^9.0",
      "blazervel/blazervel": "0.0.x-dev",
      "laravel/fortify": "^1.13"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3"
  },
  "autoload": {
      "psr-4": {
          "Blazervel\\Auth\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Blazervel\\Auth\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Blazervel\\Auth\\Providers\\BlazervelAuthServiceProvider"
          ]
      }
  }
}